<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>System.Security.Cryptography.xxHash</RootNamespace>
    <AssemblyName>xxHash4net</AssemblyName>
    <TargetFrameworks>netstandard2.0;netstandard1.3;net40</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>xxHash4net</PackageId>
    <Product>xxHash4net</Product>
    <Authors>ailen0ada</Authors>
    <Description>C# implementation of xxhash based on System.Security.Cryptography.HashAlgorithm</Description>
    <PackageProjectUrl>https://github.com/ailen0ada/xxHash4net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ailen0ada/xxHash4net</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>xxhash hashalgorithm</PackageTags>
    <Version>1.2.0</Version>
  </PropertyGroup>


</Project>